<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>PLANTER website</display-name>
	<description>PLANTER website</description>
        
         <!-- Listeners -->

    
    
               
        <servlet>
                <servlet-name>SearchTree</servlet-name>
                <display-name/>
                <description/>
                <servlet-class>edu.usc.ksom.pm.panther.pantherdb.servlet.SearchTree</servlet-class>
        </servlet>
        <servlet-mapping>
                <servlet-name>SearchTree</servlet-name>
                <url-pattern>/searchTree</url-pattern>
        </servlet-mapping>

	<session-config>
		<session-timeout>20</session-timeout>
	</session-config>
	<mime-mapping>
		<extension>txt</extension>
		<mime-type>text/plain</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>html</extension>
		<mime-type>text/html</mime-type>
	</mime-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<!-- Error pages -->
	<error-page>
		<error-code>404</error-code>
		<location>/404.jsp</location>
	</error-page>
	<!-- <error-page>
    <error-code>500</error-code>
    <location>/500.jsp</location>
  </error-page>
 -->

</web-app>
